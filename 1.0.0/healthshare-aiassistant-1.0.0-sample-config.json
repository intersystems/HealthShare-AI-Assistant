        ],
        "sslConfigs": [
            {
                "name": "HS.Secure.Demo",
                "description": "For secure connection among HealthShare components",
                "verifyPeer": true,
                "certificateFile": "/home/AutoAll/certs/active.crt",
                "caFile": "/home/AutoAll/certs/ca.crt",
                "privateKeyFile": "/home/AutoAll/certs/active.key",
                "privateKeyType": "RSA",
                "privateKeyPassword": "",
                "tlsMaxVersion": "TLSv1.3",
                "tlsMinVersion": "TLSv1.2",
                "cipherList": "ALL:!aNULL:!eNULL:!EXP:!SSLv2",
                "cipherSuites": "TLS_AES_256_GCM_SHA384:TLS_CHACHA20_POLY1305_SHA256:TLS_AES_128_GCM_SHA256",
                "ocsp": false
            },
            {
                "name": "HS.OAuth.SSLConfig",
                "description": "SSL Configuration Name used throughout HealthShare",
                "verifyPeer": false,
                "privateKeyType": "RSA",
                "tlsMaxVersion": "TLSv1.3",
                "tlsMinVersion": "TLSv1.2",
                "cipherList": "ALL:!aNULL:!eNULL:!EXP:!SSLv2",
                "cipherSuites": "TLS_AES_256_GCM_SHA384:TLS_CHACHA20_POLY1305_SHA256:TLS_AES_128_GCM_SHA256"
            }
        ],
        "httpServices": [
            {
                "name": "HSREGISTRY",
                "host": "<UCR_FQDN>",
