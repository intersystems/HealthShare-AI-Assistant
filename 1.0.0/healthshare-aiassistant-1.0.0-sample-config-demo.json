{
    "healthshare_ai_assistant": {
        "interopCredentials": [
            {
                "identifier": "HS_Services",
                "userName": "HS_Services",
                "password": "HS_Services"
            }
        ],
        "sslConfigs": [
            {
                "name": "HS.Secure.Demo",
                "description": "For secure connection among HealthShare components",
                "verifyPeer": true,
                "certificateFile": "/home/AutoAll/certs/active.crt",
                "caFile": "/home/AutoAll/certs/ca.crt",
                "privateKeyFile": "/home/AutoAll/certs/active.key",
                "privateKeyType": "RSA",
                "tlsMaxVersion": "TLSv1.3",
                "tlsMinVersion": "TLSv1.2"
            },
            {
                "name": "HS.OAuth.SSLConfig",
                "description": "SSL Configuration Name used throughout HealthShare",
                "verifyPeer": false,
                "privateKeyType": "RSA",
                "tlsMaxVersion": "TLSv1.3",
                "tlsMinVersion": "TLSv1.2"
            }
        ],
        "httpServices": [
            {
                "name": "HSREGISTRY",
                "host": "sample.host.com",
                "sslConfig": "HS.Secure.Demo",
                "url": "/ucr_instance/api/healthshare-rest/hsregistry",
                "httpCredentialsConfig": "HS_Services"
            },
            {
                "name": "sample.host.com:HSACCESS",
                "host": "sample.host.com",
                "sslConfig": "HS.Secure.Demo",
                "url": "/ucr_instance/api/healthshare-rest/hsaccess",
                "httpCredentialsConfig": "HS_Services",
                "isDefaultAccessGateway": true
            },
            {
                "name": "NavFrameRESTPrimary",
                "host": "sample.host.com",
                "sslConfig": "HS.Secure.Demo",
                "url": "/ucr_instance/api/healthshare-rest/hssys",
                "httpCredentialsConfig": "HS_Services"
            },
            {
                "name": "PatientBannerRESTServiceKey",
                "host": "sample.host.com",
                "sslConfig": "HS.Secure.Demo",
                "url": "/ucr_instance/api/healthshare-rest/hssys",
                "httpCredentialsConfig": "HS_Services"
            },            
            {
                "name": "OAuth2.Issuer",
                "host": "sample.host.com",
                "sslConfig": "HS.OAuth.SSLConfig",
                "url": "/ucr_instance/oauth2"
            }
        ],
        "soapServices": [
            {
                "name": "HSREGISTRY",
                "host": "sample.host.com",
                "sslConfig": "HS.Secure.Demo",
                "url": "/ucr_instance/csp/healthshare/hsregistry/services/HS.Hub.HSWS.WebServices.cls",
