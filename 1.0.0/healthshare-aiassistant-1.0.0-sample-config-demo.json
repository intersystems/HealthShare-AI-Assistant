            {
                "name": "HSAudit.Repository",
                "host": "sample.host.com",
                "sslConfig": "HS.Secure.Demo",
                "url": "/ucr_instance/csp/healthshare/hsregistry/services/HS.Audit.Common.WebServices.cls",
                "usernameTokenProfileConfig": "HS_Services"
            }            
        ]        
