            {
                "name": "sample.host.com:HSACCESS",
                "host": "sample.host.com",
                "sslConfig": "HS.Secure.Demo",
                "url": "/ucr_instance/csp/healthshare/hsaccess/services/HS.Gateway.Access.WebServices.cls",
                "usernameTokenProfileConfig": "HS_Services"
