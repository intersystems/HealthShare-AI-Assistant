{
    "healthshare_ai_assistant": {
        "interopCredentials": [
            {
                "identifier": "HS_Services",
                "userName": "HS_Services",
                "password": "HS_Services"
            }
        ],
        "sslConfigs": [
            {
                "name": "HS.Secure.Demo",
                "description": "For secure connection among HealthShare components",
                "verifyPeer": true,
                "certificateFile": "<path-to-file>/active.crt",
                "caFile": "<path-to-file>/ca.crt",
                "privateKeyFile": "<path-to-file>/active.key",
                "privateKeyType": "RSA",
                "tlsMaxVersion": "TLSv1.3",
                "tlsMinVersion": "TLSv1.2"
            },
            {
                "name": "HS.OAuth.SSLConfig",
                "description": "SSL Configuration Name used throughout HealthShare",
                "verifyPeer": false,
                "privateKeyType": "RSA",
                "tlsMaxVersion": "TLSv1.3",
                "tlsMinVersion": "TLSv1.2"
            }
        ],
        "httpServices": [
            {
                "name": "HSREGISTRY",
                "host": "<ucr-fully-qualified-domain-name>",
                "sslConfig": "HS.Secure.Demo",
                "url": "/<ucr-web-server-prefix-if-configured>/api/healthshare-rest/hsregistry",
                "httpCredentialsConfig": "HS_Services"
            },
            {
                "name": "<ucr-fully-qualified-domain-name>:HSACCESS",
                "host": "<ucr-fully-qualified-domain-name>",
                "sslConfig": "HS.Secure.Demo",
                "url": "/<ucr-web-server-prefix-if-configured>/api/healthshare-rest/hsaccess",
                "httpCredentialsConfig": "HS_Services",
                "isDefaultAccessGateway": true
            },
            {
                "name": "NavFrameRESTPrimary",
                "host": "<ucr-fully-qualified-domain-name>",
                "sslConfig": "HS.Secure.Demo",
                "url": "/<ucr-web-server-prefix-if-configured>/api/healthshare-rest/hssys",
                "httpCredentialsConfig": "HS_Services"
            },
            {
                "name": "PatientBannerRESTServiceKey",
                "host": "<ucr-fully-qualified-domain-name>",
                "sslConfig": "HS.Secure.Demo",
                "url": "/<ucr-web-server-prefix-if-configured>/api/healthshare-rest/hssys",
                "httpCredentialsConfig": "HS_Services"
            },            
            {
                "name": "OAuth2.Issuer",
                "host": "<ucr-fully-qualified-domain-name>",
                "sslConfig": "HS.OAuth.SSLConfig",
                "url": "/<ucr-web-server-prefix-if-configured>/oauth2"
            }
        ],
        "soapServices": [
            {
                "name": "HSREGISTRY",
                "host": "<ucr-fully-qualified-domain-name>",
                "sslConfig": "HS.Secure.Demo",
                "url": "/<ucr-web-server-prefix-if-configured>/csp/healthshare/hsregistry/services/HS.Hub.HSWS.WebServices.cls",
                "usernameTokenProfileConfig": "HS_Services"
            },
            {
                "name": "<ucr-fully-qualified-domain-name>:HSACCESS",
                "host": "<ucr-fully-qualified-domain-name>",
                "sslConfig": "HS.Secure.Demo",
                "url": "/<ucr-web-server-prefix-if-configured>/csp/healthshare/hsaccess/services/HS.Gateway.Access.WebServices.cls",
                "usernameTokenProfileConfig": "HS_Services"
            },
            {
                "name": "HSAudit.Repository",
                "host": "<ucr-fully-qualified-domain-name>",
                "sslConfig": "HS.Secure.Demo",
                "url": "/<ucr-web-server-prefix-if-configured>/csp/healthshare/hsregistry/services/HS.Audit.Common.WebServices.cls",
                "usernameTokenProfileConfig": "HS_Services"
            }            
        ]        
    }
}
