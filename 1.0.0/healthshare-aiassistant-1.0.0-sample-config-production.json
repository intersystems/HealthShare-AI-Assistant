{
    "healthshare_ai_assistant": {
        "interopCredentials": [
            {
                "identifier": "HS_Services",
                "userName": "HS_Services",
                "password": "<hs-services-password>"
            }
        ],
        "sslConfigs": [
            {
                "name": "<ssl-config-name>",
                "description": "For secure connection among HealthShare components",
                "verifyPeer": true,
                "certificateFile": "<path-to-ssl-cert-and-filename>",
                "caFile": "<path-to-ca-cert-and-filename>",
                "privateKeyFile": "<path-to-private-key-and-filename>",
                "privateKeyType": "RSA",
                "tlsMaxVersion": "TLSv1.3",
                "tlsMinVersion": "TLSv1.2"
            },
            {
                "name": "HS.OAuth.SSLConfig",
                "description": "SSL Configuration Name used throughout HealthShare",
                "verifyPeer": false,
                "privateKeyType": "RSA",
                "tlsMaxVersion": "TLSv1.3",
                "tlsMinVersion": "TLSv1.2"
            }
        ],
        "httpServices": [
            {
                "name": "<REGISTRY-NAMESPACE-UPPERCASE>",
                "host": "<ucr-fully-qualified-domain-name>",
                "sslConfig": "<ssl-config-name>",
                "url": "/<ucr-instance-name-lowercase>/api/healthshare-rest/<registry-namespace-lowercase>",
                "httpCredentialsConfig": "<hs-services-identifier>"
            },
            {
                "name": "<ucr-fully-qualified-domain-name>:<ACCESS-GATEWAY-NAMESPACE-UPPERCASE>",
                "host": "<ucr-fully-qualified-domain-name>",
                "sslConfig": "<ssl-config-name>",
                "url": "/<ucr-instance-name-lowercase>/api/healthshare-rest/<access-gateway-namespace-lowercase>",
                "httpCredentialsConfig": "<hs-services-identifier>",
                "isDefaultAccessGateway": true
            },
            {
                "name": "NavFrameRESTPrimary",
                "host": "<ucr-fully-qualified-domain-name>",
                "sslConfig": "<ssl-config-name>",
                "url": "/<ucr-instance-name-lowercase>/api/healthshare-rest/hssys",
                "httpCredentialsConfig": "<hs-services-identifier>"
            },
            {
                "name": "PatientBannerRESTServiceKey",
                "host": "<ucr-fully-qualified-domain-name>",
                "sslConfig": "<ssl-config-name>",
                "url": "/<ucr-instance-name-lowercase>/api/healthshare-rest/hssys",
                "httpCredentialsConfig": "<hs-services-identifier>"
            },            
            {
                "name": "OAuth2.Issuer",
                "host": "<ucr-fully-qualified-domain-name>",
                "sslConfig": "HS.OAuth.SSLConfig",
                "url": "/<ucr-instance-name-lowercase>/oauth2"
            }
        ],
        "soapServices": [
            {
                "name": "<REGISTRY-NAMESPACE-UPPERCASE>",
                "host": "<ucr-fully-qualified-domain-name>",
                "sslConfig": "<ssl-config-name>",
                "url": "/<ucr-instance-name-lowercase>/csp/healthshare/<registry-namespace-lowercase>/services/HS.Hub.HSWS.WebServices.cls",
                "usernameTokenProfileConfig": "<hs-services-identifier>"
            },
            {
                "name": "<ucr-fully-qualified-domain-name>:<ACCESS-GATEWAY-NAMESPACE-UPPERCASE>",
                "host": "<ucr-fully-qualified-domain-name>",
                "sslConfig": "<ssl-config-name>",
                "url": "/<ucr-instance-name-lowercase>/csp/healthshare/<access-gateway-namespace-lowercase>/services/HS.Gateway.Access.WebServices.cls",
                "usernameTokenProfileConfig": "<hs-services-identifier>"
            },
            {
                "name": "HSAudit.Repository",
                "host": "<ucr-fully-qualified-domain-name>",
                "sslConfig": "<ssl-config-name>",
                "url": "/<ucr-instance-name-lowercase>/csp/healthshare/<registry-namespace-lowercase>/services/HS.Audit.Common.WebServices.cls",
                "usernameTokenProfileConfig": "<hs-services-identifier>"
            }            
        ]        
    }
}
